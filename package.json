{
  "name": "auto-dev-vscode",
  "displayName": "auto-dev-vscode",
  "version": "0.0.1",
  "packageManager": "pnpm@8.6.6",
  "description": "Put the most loved ChatGPT into your vscode, and have things done quickly",
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.77.0"
  },
  "activationEvents": [
    "onLanguage:c",
    "onLanguage:cpp",
    "onLanguage:csharp",
    "onLanguage:go",
    "onLanguage:java",
    "onLanguage:javascript",
    "onLanguage:kotlin",
    "onLanguage:typescript",
    "onLanguage:python",
    "onLanguage:rust",
    "onLanguage:swift",
    "onLanguage:zig",
    "onLanguage:lua"
  ],
  "contributes": {
    "commands": [
      {
        "command": "auto-dev-vscode.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "build": "tsup src/extension.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "vscode:prepublish": "nr build",
    "preinstall": "npx only-allow pnpm",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp --commit --push --tag && nr publish"
  },
  "dependencies": {
    "@unit-mesh/treesitter-artifacts": "^1.4.0",
    "web-tree-sitter": "^0.20.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.7",
    "@antfu/ni": "^0.21.4",
    "@babel/types": "^7.22.5",
    "@types/node": "^20.3.3",
    "@types/vscode": "^1.79.1",
    "@vscode/vsce": "^2.19.0",
    "bumpp": "^9.1.1",
    "eslint": "^8.44.0",
    "esno": "^0.16.3",
    "pnpm": "8.6.6",
    "rimraf": "^5.0.1",
    "tsup": "^7.1.0",
    "typescript": "5.0.4",
    "vite": "^4.3.9",
    "vitest": "^0.32.4"
  }
}
