{
  "name": "autodev",
  "displayName": "AutoDev - ",
  "description": " üßô‚ÄçAutoDev: The AI-powered coding wizard with multilingual support üåê, auto code generation üèóÔ∏è, and a helpful bug-slaying assistant üêû! Customizable prompts üé® and a magic Auto Dev/Testing/Document/Agent feature üß™ included! üöÄ ",
  "version": "0.0.1",
  "repository": "https://github.com/unit-mesh/auto-dev-vscode",
  "publisher": "Phodal",
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "bugs": {
    "url": "https://github.com/unit-mesh/auto-dev-vscode/issues"
  },
  "homepage": "https://editor.unitmesh.cc",
  "qna": "https://github.com/unit-mesh/auto-dev-vscode/issues/new/choose",
  "license": "Apache-2.0",
  "pricing": "Free",
  "icon": "images/pluginIcon.png",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages",
    "Education",
    "Machine Learning",
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:java",
    "onLanguage:c",
    "onLanguage:cpp",
    "onLanguage:csharp",
    "onLanguage:go",
    "onLanguage:java",
    "onLanguage:javascript",
    "onLanguage:kotlin",
    "onLanguage:typescript",
    "onLanguage:python",
    "onLanguage:rust",
    "onStartupFinished",
    "onView:autodevGUIView"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "title": "AutoDev",
    "properties": {
      "autodev.remoteConfigServerUrl": {
        "type": "string",
        "default": null,
        "markdownDescription": "If your team is set up to use shared configuration, enter the server URL here and your user token below to enable automatic syncing."
      }
    },
    "commands": [
      {
        "command": "autodev.newSession",
        "category": "AutoDev",
        "title": "New Session",
        "icon": "$(add)",
        "group": "AutoDev"
      },
      {
        "command": "autodev.debugTerminal",
        "category": "AutoDev",
        "title": "Debug Terminal",
        "group": "AutoDev"
      },
      {
        "command": "autodev.generateDoc",
        "category": "AutoDev",
        "title": "Auto Generate Document",
        "group": "AutoDev"
      },
      {
        "command": "autodev.genApiData",
        "category": "AutoDev",
        "title": "Generate API Data",
        "group": "AutoDev"
      },
      {
        "command": "autodev.action.quickchat",
        "category": "AutoDev",
        "title": "AutoDev Quick Chat",
        "group": "AutoDev"
      }
    ],
    "keybindings": [
      {
        "command": "autodev.debugTerminal",
        "mac": "cmd+shift+r",
        "key": "ctrl+shift+r"
      },
      {
        "command": "autodev.editor.suggestCommand",
        "mac": "cmd+shift+j",
        "key": "ctrl+shift+j"
      }
    ],
    "submenus": [
      {
        "id": "autodev.autodevSubMenu",
        "label": "AutoDev"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "autodev.debugTerminal"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "autodev",
          "title": "AutoDev",
          "icon": "images/autodev-dark.svg"
        }
      ]
    },
    "views": {
      "autodev": [
        {
          "type": "webview",
          "id": "autodev.autodevGUIView",
          "name": "",
          "visibility": "visible"
        }
      ]
    }
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "watch": "vite build --watch --mode=development",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preintegration-test": "tsc -p . --outDir out",
    "integration-test": "vscode-test",
    "integration-test:watch": "tsc -p . -w --outDir out",
    "test": "vitest --run",
    "test:watch": "vitest --silent=false",
    "vscode:prepublish": "pnpm run build"
  },
  "packageManager": "pnpm@8.15.5",
  "dependencies": {
    "@unit-mesh/treesitter-artifacts": "^1.5.0",
    "node-fetch": "^3.3.2",
    "@vitest/coverage-v8": "^1.4.0",
    "velocityjs": "^2.0.6",
    "vscode-languageclient": "^9.0.1",
    "web-tree-sitter": "^0.22.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.14",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.x",
    "@types/sinon": "^17.0.3",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9",
    "chai": "^5.1.0",
    "eslint": "^8.56.0",
    "mocha": "^10.0.1",
    "sinon": "^17.0.1",
    "typescript": "^5.4.4",
    "vite": "^5.2.8",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-dts": "^3.8.1",
    "vite-plugin-externalize-deps": "^0.8.0",
    "vite-plugin-static-copy": "^1.0.2",
    "vitest": "^1.4.0"
  }
}
